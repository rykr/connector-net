			<ComponentRef Id="DocsSrc"/>
			<ComponentRef Id="TypesSrc"/>
			<ComponentRef Id="TestSuiteSrc"/>
		</Feature>
		
		<CustomAction Id="DIRCA_TARGETDIR" Property="TARGETDIR" Value="[ProgramFilesFolder][Manufacturer]\[ProductName]" Execute="firstSequence" />
		<CustomAction Id="SetProp_CREATESTARTMENU" Property="CREATESTARTMENU" Value="1" Execute="firstSequence" />
		<CustomAction Id="SetProp_REGISTERGAC" Property="REGISTERGAC" Value="1" Execute="firstSequence" />
		
		<InstallUISequence>
			<Custom Action="DIRCA_TARGETDIR" After="ValidateProductID"><![CDATA[TARGETDIR=""]]></Custom>
